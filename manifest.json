{
    "manifest_version": 3,
    "name": "Mon Extension",
    "version": "0.0.1",
    "action": {
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "activeTab"
      ],
    "background": {
        "service_worker": "background.js"
    }
}